package {
    default_applicable_licenses: ["Android-Apache-2.0"],
}

android_app {
    name: "BusinessCard",
    // Only compile source java files in this apk.
    srcs: ["src/**/*.java"],
    sdk_version: "current",
    dex_preopt: {
        enabled: false,
    },
    optimize: {
        enabled: false,
    },
}

android_app {
    name: "BusinessCard_LowerApi",
    // Only compile source java files in this apk.
    srcs: ["src/**/*.java"],
    sdk_version: "30",
    dex_preopt: {
        enabled: false,
    },
    optimize: {
        enabled: false,
    },
}
